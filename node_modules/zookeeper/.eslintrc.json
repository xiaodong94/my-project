{
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "env": {
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "indent": ["error", 4],
        "no-console": ["error", { "allow": ["info", "warn", "error"] }],
        "no-multi-assign": 0
    }
}
